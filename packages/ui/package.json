{
  "name": "@todo-app/ui",
  "version": "0.0.0",
  "private": true,
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist"
  ],
  "exports": {
    "./styles.css": "./dist/styles.css"
  },
  "scripts": {
    "build": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "lint": "eslint src/",
    "dev": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@todo-app/eslint-config": "workspace:*",
    "@todo-app/typescript-config": "workspace:*",
    "@turbo/gen": "^2.4.4",
    "@types/node": "^22.13.10",
    "@types/react": "19.0.12",
    "typescript": "5.8.2"
  },
  "dependencies": {
    "@tailwindcss/cli": "^4.1.3",
    "@tailwindcss/postcss": "^4.0.12",
    "@todo-app/design-system": "workspace:*",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.12"
  }
}
